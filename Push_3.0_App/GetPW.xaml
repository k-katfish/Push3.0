<Window x:Class="Push_3._0_App.GetPW"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Push_3._0_App"
        mc:Ignorable="d"
        Title="Enter Credential" Height="250" Width="250">
    <Grid>
        <TextBox x:Name="UserNameTextBox" HorizontalAlignment="Left" Margin="81,40,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="140" IsEnabled="False"/>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="10,91,0,0" VerticalAlignment="Top"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="117,135,0,0" VerticalAlignment="Top" Click="OKButton_Click" Width="51"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="173,135,0,0" VerticalAlignment="Top" Width="47" Click="CancelButton_Click"/>
        <PasswordBox x:Name="PasswordPBox" HorizontalAlignment="Left" Margin="81,95,0,0" VerticalAlignment="Top" Width="140" Password=""/>
    </Grid>
</Window>
