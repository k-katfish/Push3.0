<Window x:Class="CsCredential.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CsCredential"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="350">
    <Grid>
        <TextBox x:Name="UsernameTBox" HorizontalAlignment="Left" Margin="96,59,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="200" IsEnabled="False"/>
        <Label Content="Username:" HorizontalAlignment="Left" Margin="22,55,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="22,86,0,0" VerticalAlignment="Top" Width="66"/>
        <Button x:Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="96,157,0,0" VerticalAlignment="Top" Width="140" Click="OKButton_Click"/>
        <PasswordBox x:Name="PasswordPBox" HorizontalAlignment="Left" Margin="96,90,0,0" VerticalAlignment="Top" Width="200"/>

    </Grid>
</Window>
